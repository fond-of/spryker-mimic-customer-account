{
  "name": "fond-of-spryker/mimic-customer-account",
  "description": "Mimic user accounts in Spryker",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris E",
      "email": "christopher.egger@fondof.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/checkout-extension": "^1.2.0",
    "spryker/customer": "^6.0.0 || ^7.0.0",
    "spryker/quote": "^1.0.0 || ^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "^2.4",
    "codeception/mockery-module": "^0.2",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
